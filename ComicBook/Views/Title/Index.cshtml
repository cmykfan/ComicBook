@model ComicBook.Models.TitleListViewModel

@{
    ViewBag.Title = "Titles";
}

<h2>Titles</h2>

<table class="table table-striped">
    <thread>
        <tr>
            <th>
                Name
            </th>
        </tr>
    </thread>
    <tbody>
        @for (var i = 0; i < Model.Titles.Count; i++)
        {
            <tr>
                <td>
                    @Model.Titles[i].FullName
                </td>
            </tr>
        }
    </tbody>
</table>

Total Titles: @Model.TotalTitles

